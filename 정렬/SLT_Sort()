#include <iostream>
#include <algorithm> // 대표적인 STL라이브러리 중 하나이다. 이 안에 sort() 함수가 포함되어있음.

using namespace std;

int main(void) {
      int a[10] = {9, 3, 5, 4, 1, 10, 8, 6, 7, 2}
      sort(a, a + 10) // 정렬할 배열의 첫 번째 주소와 마지막 주소를 넘겨줌.
      for(int i = 0; i < 10; i ++) {
            cout << a[i] << ' ';
      }
}
